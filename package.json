{
  "name": "casnose",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:all": "electron-vite dev",
    "dev:server": "nodemon",
    "dev:customer": "cross-env SCREEN_MODE=customer electron-vite dev",
    "dev:display": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:display-wait\"",
    "dev:display-wait": "node -e \"setTimeout(() => process.exit(0), 5000)\" && cross-env SCREEN_MODE=display electron-vite dev",
    "dev:display-only": "cross-env SCREEN_MODE=display electron-vite dev",
    "dev:window": "cross-env SCREEN_MODE=window electron-vite dev",
    "dev:admin": "cross-env SCREEN_MODE=admin electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "build:server": "tsc -p tsconfig.server-build.json",
    "build:server-exe": "npm run build:server && pkg dist-server/server/server.js --targets node18-win-x64 --output resources/server.exe",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "clean": "rimraf dist && rimraf dist-server",
    "clean:dist": "rimraf dist",
    "clean:server": "rimraf dist-server",
    "build:all-screens": "npm run clean && npm run build:server-exe && node scripts/build-all.js",
    "build:single": "node scripts/build-single.js",
    "build:display": "npm run clean && node scripts/build-single.js display",
    "build:customer": "npm run clean && node scripts/build-single.js customer",
    "build:window": "npm run clean && node scripts/build-single.js window",
    "build:admin": "npm run clean && node scripts/build-single.js admin"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@floating-ui/dom": "^1.7.2",
    "@floating-ui/react": "^0.27.13",
    "@headlessui/react": "^2.2.4",
    "@react-spring/web": "^10.0.1",
    "@types/react-transition-group": "^4.4.12",
    "@use-gesture/react": "^10.3.1",
    "address": "^2.0.3",
    "axios": "^1.10.0",
    "better-sqlite3": "^11.10.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "framer-motion": "^12.23.0",
    "fs-extra": "^11.3.0",
    "helmet": "^8.1.0",
    "ip": "^2.0.1",
    "joi": "^17.13.3",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.525.0",
    "moment": "^2.30.1",
    "network": "^0.7.0",
    "node-cron": "^4.1.0",
    "puppeteer": "^24.10.2",
    "react-intersection-observer": "^9.16.0",
    "react-spring": "^10.0.1",
    "react-transition-group": "^4.4.5",
    "react-use-gesture": "^9.1.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/ip": "^1.1.3",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "wait-on": "^8.0.3"
  }
}
